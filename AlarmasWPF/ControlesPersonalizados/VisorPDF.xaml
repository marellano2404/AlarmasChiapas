<Window x:Class="AlarmasWPF.ControlesPersonalizados.VisorPDF"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AlarmasWPF.ControlesPersonalizados"
        mc:Ignorable="d"
        Title="VisorPDF" 
        d:DesignHeight="400" d:DesignWidth="800" >

        <DockPanel>
            <Menu DockPanel.Dock="Top">
                <MenuItem Header="_Archivo">
                    <MenuItem Header="_Abrir" Click="AbrirArchivo_Click" />
                </MenuItem>
                <MenuItem Header="_Guardar" Click="GuardarArchivo_Click"></MenuItem>
        </Menu>
            <DocumentViewer x:Name="DocumentViewer"/>
        </DockPanel>
</Window>
